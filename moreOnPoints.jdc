class Point {
	double x;
	double y;
	
	double distanceTo(Point p) {
		return Math.sqrt(Math.pow(this.x - p.x, 2) + Math.pow(this.y - p.y, 2));
	}
	
	double distanceToOrigin() {
		return this.distanceTo(new Point());
	}
	
	void moveTo(double x, double y) {
		this.x = x;
		this.y = y;
	}
	
